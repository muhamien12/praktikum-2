pins = {"TioFani":1234, "Amin":1111,"Fahri":2222}
pins["TioFani"] // Menampilkan value
1234

pins.keys()
dict_keys(['TioFani', 'Amin', 'Fahri'])

pins.values()
dict_values([1234, 1111, 2222])

pins["Baru"]=4444 // menambahkan
pins
{'TioFani': 1234, 'Amin': 1111, 'Fahri': 2222, 'Baru': 4444}

pins.pop("Baru") // menghapus baru
4444
pins
{'TioFani': 1234, 'Amin': 1111, 'Fahri': 2222}
pins["Elok"]="AB12"
pins
{'TioFani': 1234, 'Amin': 1111, 'Fahri': 2222, 'Elok': 'AB12'}




